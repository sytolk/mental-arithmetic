{
  "name": "@tagspaces/arithmetic-player",
  "version": "1.0.0",
  "description": "TagSpaces extension for mental arithmetic",
  "homepage": "./",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "dev": "vite dev --host",
    "build": "tsc && vite build",
    "build-dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "zip": "npm run build && npx npm-build-zip --source=.",
    "code-format": "npx prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mental",
    "arithmetic"
  ],
  "author": "TagSpaces UG <info@tagspaces.org> (https://www.tagspaces.org)",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.2.0",
    "easy-speech": "^2.2.0",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "react-i18next": "^15.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "written-number": "github:sytolk/js-written-number"
  },
  "devDependencies": {
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.4",
    "prettier": "^2.7.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagspaces/tagspaces-extensions.git"
  },
  "bugs": {
    "url": "https://github.com/tagspaces/tagspaces-extensions/issues"
  },
  "tsextension": {
    "name": "Mental Arithmetic",
    "types": [
      "viewer",
      "editor"
    ],
    "fileTypes": [
      {
        "ext": "math",
        "color": "#5cb85c"
      }
    ],
    "buildFolder": "build"
  }
}
